{
  "name": "@fhevm/react-sdk",
  "version": "1.0.0",
  "description": "React hooks and components for FHEVM",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "ethers": "^6.0.0",
    "wagmi": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "wagmi": {
      "optional": true
    }
  },
  "dependencies": {
    "@fhevm/sdk": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "react": "^18.3.1",
    "typescript": "^5.8.2",
    "tsup": "^8.0.0"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "react",
    "hooks",
    "encryption",
    "zama"
  ],
  "license": "BSD-3-Clause-Clear"
}

